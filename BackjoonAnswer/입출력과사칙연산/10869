// https://www.acmicpc.net/problem/10869

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		int A, B;
		Scanner sc = new Scanner(System.in);
		while(true) {
			A = sc.nextInt();
			B = sc.nextInt();
			if(1 <= A && B <= 10000) {
				System.out.println(A + B);
				System.out.println(A - B);
				System.out.println(A * B);
				System.out.println(A / B);
				System.out.println(A % B);
				break;
			}
		}
	}
}
