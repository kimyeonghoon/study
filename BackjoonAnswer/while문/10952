// https://www.acmicpc.net/problem/10952

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int A, B;
		while(true) {
			A = sc.nextInt();
			B = sc.nextInt();
			if(A == 0 && B == 0) {
				break;
			} else {
				System.out.println(A + B);
			}
		}
	}
}
