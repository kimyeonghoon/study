// https://www.acmicpc.net/problem/2739

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		while(true) {
			boolean endFlag = false;
			if(N >= 1 && N <= 9) {
				for(int i = 1; i <= 9; i++) {
					System.out.println(N + " * " + i + " = " + N * i);
				}
				endFlag = true;
			} else {
				N = sc.nextInt();
			}
			if(endFlag) {
				break;
			}
		}
	}
}
